{
  "openapi": "3.1.0",
  "info": {
    "version": "1.0.0",
    "title": "OpenAPI"
  },
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/api/upload": {
      "post": {
        "tags": [
          "Uploads"
        ],
        "summary": "Generate a pre-signed URL for uploading a file",
        "operationId": "post_UploadEndpoint",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "image/jpeg",
                      "image/png",
                      "image/gif"
                    ]
                  },
                  "size": {
                    "type": "number",
                    "maximum": 1048576
                  },
                  "authorUUID": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "moduleUUID": {
                    "type": "string",
                    "format": "uuid"
                  }
                },
                "required": [
                  "name",
                  "type",
                  "size",
                  "authorUUID",
                  "moduleUUID"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Returns the pre-signed URL",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "uploadUrl": {
                          "type": "string"
                        },
                        "objectKey": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "uploadUrl",
                        "objectKey"
                      ]
                    }
                  },
                  "required": [
                    "success",
                    "result"
                  ]
                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "success",
                    "error"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/api/db_test": {
      "get": {
        "tags": [
          "DB Test"
        ],
        "summary": "Test database connectivity",
        "operationId": "get_DBTestEndpoint",
        "responses": {
          "200": {
            "description": "Database connectivity test passed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    }
                  },
                  "required": [
                    "success"
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "webhooks": {}
}